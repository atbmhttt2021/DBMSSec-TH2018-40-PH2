<script>
  $('#logoutBtn').on( 'click', function (e) {
    e.preventDefault();
    $.ajax({
      url: `/auth/logout`,
      type: 'GET',
    }).done(function (data) {
      window.location.replace("/auth/login");
    }).fail(function (error) {
      Swal.fire({
          title: 'Lỗi!',
          icon: 'error',
          confirmButtonText: 'Cool'
      })
    });
  });
</script>